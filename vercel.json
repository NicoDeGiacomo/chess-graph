{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/repertoire/:id", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/screenshots/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/(favicon.*|logo.*|apple-touch-icon.*|manifest.json)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=86400" }]
    }
  ]
}
